{
  "name": "ielm",
  "version": "0.1.0",
  "description": "Interactive Elm",
  "main": "index.js",
  "dependencies": {
    "array-unique": "^0.3.2",
    "codemirror": "^5.32.0",
    "exit-hook": "^1.1.1",
    "node-elm-repl": "^3.5.0",
    "node-simplehttpserver": "^0.2.7",
    "recursive-copy": "^2.0.7",
    "rimraf": "^2.6.2",
    "webpack": "^3.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "css-loader": "^0.28.7",
    "ignore-loader": "^0.1.2",
    "style-loader": "^0.19.0",
    "webpack-dev-server": "^2.9.1"
  },
  "scripts": {
    "build": "webpack",
    "test-build": "npm run clean-output && npm run create-output-dir && npm run quick-test-build",
    "quick-test-build": "npm run build && npm run copy-components && npm run copy-elm-package && npm run install-packages && npm run start-server & npm run start-client",
    "create-output-dir": "mkdir ./output",
    "clean-output": "rm -Rf ./output",
    "copy-components": "cp -R ./src/server/Component ./output",
    "copy-elm-package": "cp ./src/server/elm-package.sample.json ./output/elm-package.json",
    "install-packages": "cd ./output && elm-package install --yes && cd ..",
    "start-server": "node ./src/server/server.js",
    "start-client": "./node_modules/.bin/node-simplehttpserver . 8080",
    "start-dev-client": "./node_modules/.bin/webpack-dev-server",
    "quick-start": "npm run copy-components && npm run copy-elm-package && npm run install-packages && npm run start-server & npm run start-dev-client",
    "start": "npm run clean-output && npm run create-output-dir && npm run quick-start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shamansir/ielm.git"
  },
  "author": "Ulric Wilfred <shaman.sir@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shamansir/ielm/issues"
  },
  "homepage": "https://github.com/shamansir/ielm#readme"
}
